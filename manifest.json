{
  "name": "Fontcycler",
  "description": "Preview fonts on current page",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Fontcycler"
  },
  "manifest_version": 2
}
